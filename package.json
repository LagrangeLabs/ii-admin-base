{
  "name": "ii-admin-base",
  "version": "1.3.0",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "description": "A library of react components, which mainly stores components that can be reused by all business lines of AI-Indeeded Company.",
  "author": "ShiMu",
  "license": "MIT",
  "keywords": [
    "React",
    "Component"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "homepage": "https://lagrangelabs.github.io/ii-admin-base",
  "repository": {
    "type": "git",
    "url": "https://github.com/LagrangeLabs/ii-admin-base.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:nowatch && npm run lint"
    }
  },
  "dependencies": {
    "@types/react-slick": "^0.23.4",
    "codemirror": "^5.59.0",
    "react-slick": "^0.28.1",
    "slick-carousel": "^1.8.1"
  },
  "peerDependencies": {
    "@ant-design/icons": ">=4.2.0",
    "antd": ">=4.7.0",
    "classnames": ">=2.2.0",
    "lodash": ">=4.17.0",
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.2.2",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "@umijs/test": "^3.0.5",
    "antd": "^4.13.1",
    "classnames": "^2.2.6",
    "dumi": "^1.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.20",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "yorkie": "^2.0.0"
  }
}
